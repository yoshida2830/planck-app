# Planck Environment Variables
# Copy this file to .env.local and fill in your values

# API Keys (Mock - replace with real values in production)
NEXT_PUBLIC_API_URL=https://api.planck.com
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_EARTH_ENGINE_API_KEY=your_earth_engine_api_key_here

# Database (Mock - replace with real database URL)
DATABASE_URL=postgresql://username:password@localhost:5432/planck_db

# Auth (Mock - replace with real auth provider)
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_here
DISCORD_CLIENT_ID=your_discord_client_id_here
DISCORD_CLIENT_SECRET=your_discord_client_secret_here

# Analytics (Optional)
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
POSTHOG_KEY=your_posthog_key_here

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_COMMUNITY=true
NEXT_PUBLIC_ENABLE_PREMIUM_FEATURES=false

# Development
NODE_ENV=development
NEXT_PUBLIC_APP_VERSION=0.1.0
